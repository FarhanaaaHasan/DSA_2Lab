//nth fibonacci number:
__________________________________________

#include<bits/stdc++.h>
using namespace std;
int fibona(int n){
    if(n==0){
        return 0;
    }
    if(n==1){
        return 1;
    }
    else{
        return fibona(n-1)+ fibona(n-2);
    }
}

int main(){
    int n;
    cin>>n;
    printf("%dth fibonacci number is %d ", n,fibona(n));


return 0;
}
